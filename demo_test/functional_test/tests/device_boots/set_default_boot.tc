./dm attach $1:$2:120
model=`./dm getredfishmodel $1:$2:""|sed 's/ /_/'`
[ "$model" == "Redfish_PSME" ] && ./dm setsessionservice $1:$2:"":true:300
token=`./dm logindevice $1:$2:$5:$6| cut -d' ' -f4`
model=`./dm getredfishmodel $1:$2:$token|sed 's/ /_/'`
if [ "$model" == "Redfish_PSME" ]; then
bootdata=`./dm getsystembootdata $1:$2:$token`
systembootdata=`echo $bootdata | sed -e 's/\[//g' -e 's/\]//g' -e 's/(//g' -e 's/)//g' -e 's/:/ /g' -e 's/ //g'`
echo $systembootdata
if [ $2 == "8888" ]; then
os=`./dm getdefaultboot $1:$2:$token`
os=`echo $os|sed -e 's/ /-/g'`
[ "$os" == "Open-Network-Linux" ] && os_output1=$os || os_output1=""
echo $os_output1
./dm setdefaultboot $1:$2:$token:linux
os=`./dm getdefaultboot $1:$2:$token`
os=`echo $os|sed -e 's/ /-/g'`
[ "$os" == "Open-Network-Linux" ] && os_output2=$os || os_output2=""
echo $os_output2
./dm setdefaultboot $1:$2:$token:update
os=`./dm getdefaultboot $1:$2:$token`
[ "$os" == "ONIE:update" ] && os_output3=$os || os_output3=""
echo $os_output3
./dm setdefaultboot $1:$2:$token:open
os=`./dm getdefaultboot $1:$2:$token`
os=`echo $os|sed -e 's/ /-/g'`
[ "$os" == "Open-Network-Linux" ] && os_output4=$os || os_output4=""
echo $os_output4
elif [ $2 == "8889" ];then
os=`./dm getdefaultboot $1:$2:$token`
[ "$os" == "SONiC-OS" ] && os_output1=$os || os_output1=""
echo $os_output1
./dm setdefaultboot $1:$2:$token:linux
os=`./dm getdefaultboot $1:$2:$token`
[ "$os" == "SONiC-OS" ] && os_output2=$os || os_output2=""
echo $os_output2
./dm setdefaultboot $1:$2:$token:update
os=`./dm getdefaultboot $1:$2:$token`
[ "$os" == "ONIE:update" ] && os_output3=$os || os_output3=""
echo $os_output3
./dm setdefaultboot $1:$2:$token:sonic
os=`./dm getdefaultboot $1:$2:$token`
[ "$os" == "SONiC-OS" ] && os_output4=$os || os_output4=""
echo $os_output4
fi
fi
./dm detach $1:$2:$token

sed -e '/^\/\//d' -e 's/ip1/'${1}'/g; s/port1/'${2}'/g; s/ip2/'${3}'/g; s/port2/'${4}'/g; s/systembootdata/'${systembootdata}'/g; s/os1/'${os_output1}'/g; s/os2/'${os_output2}'/g; s/os3/'${os_output3}'/g; s/os4/'${os_output4}'/g' "$9".expected${model} > "$9".e
