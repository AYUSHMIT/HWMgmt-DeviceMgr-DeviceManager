./dm attach $1:$2:120
model=`./dm getredfishmodel $1:$2:""|sed 's/ /_/'`
[ "$model" == "Redfish_PSME" ] && ./dm setsessionservice $1:$2:"":true:300
token=`./dm logindevice $1:$2:$5:$6| cut -d' ' -f4`
model=`./dm getredfishmodel $1:$2:$token|sed 's/ /_/'`
if [ "$model" == "Redfish_PSME" ]; then
memorystatus=`./dm getmemorystatus $1:$2:$token`
status1=`echo $memorystatus|awk -F"[,:}]" '{print $2}'`
lower1=`echo $memorystatus|awk -F"[,:}]" '{print $4}'`
echo $memorystatus
./dm setmemorystatus $1:$2:$token:84
memorystatus=`./dm getmemorystatus $1:$2:$token`
./dm setmemorystatus $1:$2:$token:0
memorystatus=`./dm getmemorystatus $1:$2:$token`
status2=`echo $memorystatus|awk -F"[,:}]" '{print $2}'`
lower2=`echo $memorystatus|awk -F"[,:}]" '{print $4}'`
echo $memorystatus
./dm setmemorystatus $1:$2:$token:100
memorystatus=`./dm getmemorystatus $1:$2:$token`
status3=`echo $memorystatus|awk -F"[,:}]" '{print $2}'`
lower3=`echo $memorystatus|awk -F"[,:}]" '{print $4}'`
echo $memorystatus
fi
./dm detach $1:$2:$token

sed -e '/^\/\//d' -e 's/ip1/'${1}'/g; s/port1/'${2}'/g; s/ip2/'${3}'/g; s/port2/'${4}'/g; s/status1/'${status1}'/g; s/lower1/'${lower1}'/g; s/status2/'${status2}'/g; s/lower2/'${lower2}'/g; s/status3/'${status3}'/g; s/lower3/'${lower3}'/g' "$9".expected${model} > "$9".e
