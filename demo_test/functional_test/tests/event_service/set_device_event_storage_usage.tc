./dm attach $1:$2:120
model=`./dm getredfishmodel $1:$2:""|sed 's/ /_/'`
[ "$model" == "Redfish_PSME" ] && ./dm setsessionservice $1:$2:"":true:300
token=`./dm logindevice $1:$2:$5:$6| cut -d' ' -f4`
model=`./dm getredfishmodel $1:$2:$token|sed 's/ /_/'`
if [ "$model" == "Redfish_PSME" ]; then
storagestatus=`./dm getstoragestatus $1:$2:$token`
loading1=`echo $storagestatus|awk -F"[,:}]" '{print $2}'`
upper1=`echo $storagestatus|awk -F"[,:}]" '{print $4}'`
echo $storagestatus
./dm setstoragestatus $1:$2:$token:84
storagestatus=`./dm getstoragestatus $1:$2:$token`
./dm setstoragestatus $1:$2:$token:0
storagestatus=`./dm getstoragestatus $1:$2:$token`
loading2=`echo $storagestatus|awk -F"[,:}]" '{print $2}'`
upper2=`echo $storagestatus|awk -F"[,:}]" '{print $4}'`
echo $storagestatus
./dm setstoragestatus $1:$2:$token:100
storagestatus=`./dm getstoragestatus $1:$2:$token`
loading3=`echo $storagestatus|awk -F"[,:}]" '{print $2}'`
upper3=`echo $storagestatus|awk -F"[,:}]" '{print $4}'`
echo $storagestatus
fi
./dm detach $1:$2:$token

sed -e '/^\/\//d' -e 's/ip1/'${1}'/g; s/port1/'${2}'/g; s/ip2/'${3}'/g; s/port2/'${4}'/g; s/loading1/'${loading1}'/g; s/upper1/'${upper1}'/g; s/loading2/'${loading2}'/g; s/upper2/'${upper2}'/g; s/loading3/'${loading3}'/g; s/upper3/'${upper3}'/g' "$9".expected${model} > "$9".e
