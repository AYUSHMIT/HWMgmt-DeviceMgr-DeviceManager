./dm attach $1:$2:120
model=`./dm getredfishmodel $1:$2:""|sed 's/ /_/'`
[ "$model" == "Redfish_PSME" ] && ./dm setsessionservice $1:$2:"":true:300
admin_token=`./dm logindevice $1:$2:$5:$6| cut -d' ' -f4`
model=`./dm getredfishmodel $1:$2:$admin_token|sed 's/ /_/'`
account_data=`./dm deviceaccountslist $1:$2:$admin_token|sed 's/\( \)*/\1/g'`
echo $account_data
./dm createaccount $1:$2:$admin_token:admin_1:0penBmc:Administrator
./dm createaccount $1:$2:$admin_token:operator:0penBmc:Operator
if [ "$model" == "Redfish_PSME" ]; then
./dm createaccount $1:$2:$admin_token:readonly:0penBmc:ReadOnlyUser
elif [ "$model" == "OpenBmc" ]; then
./dm createaccount $1:$2:$admin_token:readonly:0penBmc:User
fi
account_data=`./dm deviceaccountslist $1:$2:$admin_token|sed 's/\( \)*/\1/g'`
echo $account_data
admin_1_token=`./dm logindevice $1:$2:admin_1:0penBmc| cut -d' ' -f4`
operator_token=`./dm logindevice $1:$2:operator:0penBmc| cut -d' ' -f4`
readonly_token=`./dm logindevice $1:$2:readonly:0penBmc| cut -d' ' -f4`
account_data=`./dm deviceaccountslist $1:$2:$admin_token|sed 's/\( \)*/\1/g'`
echo $account_data
./dm deleteaccount $1:$2:$admin_token:readonly
./dm deleteaccount $1:$2:$admin_token:operator
./dm deleteaccount $1:$2:$admin_token:admin_1
./dm detach $1:$2:$admin_token

sed -e '/^\/\//d' -e 's/ip1/'${1}'/g; s/port1/'${2}'/g; s/ip2/'${3}'/g; s/port2/'${4}'/g; s/admin_token/'${admin_token}'/g; s/admin_1_token/'${admin_1_token}'/g; s/operator_token/'${operator_token}'/g; s/readonly_token/'${readonly_token}'/g' "$9".expected${model} > "$9".e
