./dm attach $1:$2:120 $3:$4:100
model_1=`./dm getredfishmodel $1:$2:""|sed 's/ /_/'`
[ "$model_1" == "Redfish_PSME" ] && set_output_1=`./dm setsessionservice $1:$2:"":true:300`
token_1=`./dm logindevice $1:$2:$5:$6| cut -d' ' -f4`
model_1=`./dm getredfishmodel $1:$2:$token_1|sed 's/ /_/'`
model_2=`./dm getredfishmodel $3:$4:""|sed 's/ /_/'`
[ "$model_2" == "Redfish_PSME" ] && set_output_2=`./dm setsessionservice $3:$4:"":true:300`
token_2=`./dm logindevice $3:$4:$7:$8| cut -d' ' -f4`
model_2=`./dm getredfishmodel $3:$4:$token_2|sed 's/ /_/'`
if [ $model_1 == "OpenBmc" -o $model_2 == "OpenBmc" ]; then
[ $model_1 == "OpenBmc" ] && model=$model_1
[ $model_2 == "OpenBmc" ] && model=$model_2
[ $model_1 != "OpenBmc" ] && echo $set_output_1 && ipPort="$1:$2"
[ $model_2 != "OpenBmc" ] && echo $set_output_2 && ipPort="$3:$4"
else
model=$model_1
echo $set_output_1
echo $set_output_2
fi
./dm detach $1:$2:$token_1
./dm detach $3:$4:$token_2
./dm detach $1:$2:$token_1

sed -e '/^\/\//d' -e 's/ip1/'${1}'/g; s/port1/'${2}'/g; s/ip2/'${3}'/g; s/port2/'${4}'/g; s/token/'${token}'/g; s/ipPort/'${ipPort}'/g' "$9".expected${model} > "$9".e
